rule rain_hist
when
	Item F_RAIN received update ON or
	Item F_RAIN received update 1
then
	RAIN.postUpdate(1)
end

rule rain
when
	Item RAIN received update 1
then
	sendNotification("e33b1711@gmail.com", "Its raining.")
	sendCommand(DF_OG_KN, 100)
	sendCommand(DF_OG_KS, 100)
	sendCommand(DF_OG_SZ, 100)
	sendCommand(DF_OG_GA, 100)
end
